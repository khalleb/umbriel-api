{
  "name": "umbriel-api",
  "version": "1.0.0",
  "description": "umbriel-api",
  "main": "index.js",
  "author": "khallebw@gmail.com",
  "license": "MIT",
  "scripts": {
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect=0.0.0.0:9229 --respawn --transpile-only --ignore-watch node_modules src/shared/infra/http/index.ts | pino-pretty -c",
    "dev:worker": "ts-node-dev -r tsconfig-paths/register --inspect=0.0.0.0:9230 --transpile-only --exit-child --ignore-watch node_modules src/shared/infra/queue/worker.ts | pino-pretty -c",
    "dev:webhook": "ts-node-dev -r tsconfig-paths/register --inspect=0.0.0.0:9231 --transpile-only --exit-child --ignore-watch node_modules src/shared/infra/sns-webhook/server.ts | pino-pretty -c",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start:server": "NODE_ENV=production node dist/shared/infra/http/index.js",
    "start:worker": "NODE_ENV=production node dist/shared/infra/queue/worker.js",
    "start:webhook": "NODE_ENV=production node dist/shared/infra/sns-webhook/server.js",
    "typeorm": "ts-node-dev -r tsconfig-paths/register --transpile-only ./node_modules/typeorm/cli.js",
    "typeorm-run": "yarn typeorm migration:run",
    "style": "npx eslint --fix . && npx prettier --write .",
    "seed:config": "ts-node-dev -r tsconfig-paths/register --transpile-only ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node-dev -r tsconfig-paths/register --transpile-only ./node_modules/typeorm-seeding/dist/cli.js seed",
    "update-version": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/devops/updateVersion.ts | pino-pretty -c",
    "make:usecase": "plop --plopfile ./.plop/usecase.ts"
  },
  "dependencies": {
    "aws-sdk": "^2.1052.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "bullmq": "^1.48.2",
    "celebrate": "^15.0.0",
    "class-transformer": "^0.5.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^4.16.3",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "envalid": "^7.2.2",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "helmet": "^5.0.1",
    "html-to-text": "^8.1.0",
    "http-status": "^1.5.0",
    "inline-css": "^3.0.0",
    "ioredis": "^4.27.9",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.1",
    "pino": "^7.6.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.41",
    "typeorm-seeding": "^1.6.1",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-proposal-decorators": "^7.16.5",
    "@babel/plugin-proposal-private-methods": "^7.16.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/babel__core": "^7.1.17",
    "@types/babel__preset-env": "^7.9.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/html-to-text": "^8.0.1",
    "@types/inline-css": "^3.0.1",
    "@types/ioredis": "^4.27.1",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.178",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.8",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.3",
    "@types/pino": "^7.0.5",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-import-helpers": "^1.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "pino-pretty": "^7.3.0",
    "prettier": "^2.4.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4",
    "plop": "2.7.4"
  }
